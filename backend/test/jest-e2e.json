{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "consts": "<rootDir>/consts",
    "utils": "<rootDir>/utils",
    "src/(.*)": "<rootDir>/src/$1"
  }
}
